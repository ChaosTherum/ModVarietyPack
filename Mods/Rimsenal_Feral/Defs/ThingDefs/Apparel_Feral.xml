<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

    
  <ThingDef ParentName="HatBase">
    <defName>Apparel_HuskerHood</defName>
    <label>feral hood</label>
    <description>A crude but rugged hood of the ferals.</description>
    <graphicData>
      <texPath>Apparel/HuskerHood</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>42</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>
    <statBases>
      <WorkToMake>6000</WorkToMake>
      <ArmorRating_Blunt>0.09</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.04</ArmorRating_Sharp>
      <Insulation_Heat>5</Insulation_Heat>
      <Insulation_Cold>-8</Insulation_Cold>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>-0.2</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <worngraphicPath>Apparel/HuskerHood</worngraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <commonality>1</commonality>
      <tags>
        <li>FeralOutfit</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>10</weight>
          <only>RGBA(0.9,0.54,0.15,1)</only>
        </li>
        <li>
          <weight>15</weight>
          <only>RGBA(0.9,0.6,0.18,1)</only>
        </li>
        <li>
          <weight>20</weight>
          <only>RGBA(0.9,0.42,0.23,1)</only>
        </li>
      </options>
    </colorGenerator>
  </ThingDef>


